<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Inscripciones de estudiantes en cursos (relaciones Many2many) -->
        <!-- Juan Pérez en Matemáticas y Física -->
        <record id="alumno_demo_1" model="school.alumno">
            <field name="curso_ids" eval="[(6, 0, [ref('curso_demo_1'), ref('curso_demo_2')])]"/>
        </record>
        
        <!-- Ana López en Física y Química -->
        <record id="alumno_demo_2" model="school.alumno">
            <field name="curso_ids" eval="[(6, 0, [ref('curso_demo_2'), ref('curso_demo_3')])]"/>
        </record>
        
        <!-- Carlos García en Matemáticas -->
        <record id="alumno_demo_3" model="school.alumno">
            <field name="curso_ids" eval="[(6, 0, [ref('curso_demo_1')])]"/>
        </record>
        
        <!-- María Rodríguez en todos los cursos -->
        <record id="alumno_demo_4" model="school.alumno">
            <field name="curso_ids" eval="[(6, 0, [ref('curso_demo_1'), ref('curso_demo_2'), ref('curso_demo_3')])]"/>
        </record>
    </data>
</openerp>